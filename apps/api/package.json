{
    "name": "@orbicheck/api",
    "version": "0.1.0",
    "type": "commonjs",
    "main": "dist/server.js",
    "scripts": {
        "dev": "ts-node-dev --require dotenv/config --transpile-only --respawn src/server.ts",
        "infra": "podman compose -f ../../infra/compose/dev.compose.yml up -d",
        "build": "tsc -p tsconfig.json",
        "start": " node dist/server.js",
        "migrate": "node-pg-migrate -m ./migrations up",
        "migrate:create": "node-pg-migrate create -m ./migrations",
        "db:init": "ts-node ./src/jobs/refreshDisposable.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "lint": "eslint . --ext .ts"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.896.0",
        "@fastify/cors": "11.1.0",
        "@fastify/swagger": "^9.5.2",
        "@fastify/swagger-ui": "^5.2.3",
        "@hapi/address": "^5.1.1",
        "@hapi/tlds": "^1.1.3",
        "@sentry/node": "^10.15.0",
        "@sinclair/typebox": "^0.34.41",
        "@types/bcryptjs": "^3.0.0",
        "@types/jsonwebtoken": "^9.0.10",
        "bcryptjs": "3.0.2",
        "bullmq": "^5.58.9",
        "dotenv": "^17.2.2",
        "fastify": "5.6.1",
        "fastify-plugin": "5.0.1",
        "ioredis": "5.8.0",
        "jsonwebtoken": "^9.0.2",
        "libphonenumber-js": "1.12.22",
        "lru-cache": "11.2.2",
        "node-cron": "^4.2.1",
        "node-fetch": "^2.7.0",
        "node-pg-migrate": "8.0.3",
        "pg": "8.16.3",
        "pino": "9.11.0",
        "postal": "^2.0.6",
        "rate-limiter-flexible": "8.0.1",
        "soap": "1.4.1",
        "tldts": "^7.0.16",
        "twilio": "^5.10.1",
        "uuid": "13.0.0",
        "zod": "4.1.11"
    },
    "devDependencies": {
        "@babel/preset-typescript": "^7.27.1",
        "@types/jest": "30.0.0",
        "@types/node": "24.5.2",
        "@types/node-cron": "^3.0.11",
        "@types/node-fetch": "^2.6.13",
        "@types/pg": "^8.15.5",
        "@types/supertest": "6.0.3",
        "@typescript-eslint/eslint-plugin": "^8.44.1",
        "@typescript-eslint/parser": "^8.44.1",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^10.1.8",
        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-deprecation": "^3.0.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-n": "^17.23.1",
        "eslint-plugin-promise": "^7.2.1",
        "eslint-plugin-regexp": "^2.10.0",
        "eslint-plugin-security": "^1.7.1",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "eslint-plugin-sonarjs": "^0.24.0",
        "eslint-plugin-unicorn": "^52.0.0",
        "jest": "30.1.3",
