{
  "service": "api",
  "port": 8080,
  "logLevel": "info",
  "database": {
    "url": "postgresql://postgres:postgres@localhost:5432/orbitcheck"
  },
  "redis": {
    "url": "redis://localhost:6379"
  },
  "s3": {
    "endpoint": "http://localhost:9000",
    "bucket": "orbitcheck"
  },
  "rateLimit": {
    "count": 30000
  },
  "retention": {
    "days": 90
  }
}